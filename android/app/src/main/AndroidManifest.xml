        </intent-filter>
         <intent-filter>
              <action android:name="android.intent.action.VIEW" />
              <category android:name="android.intent.category.DEFAULT" />
              <category android:name="android.intent.category.BROWSABLE" />
              <data android:scheme="msauth" android:host="com.mwa"   android:pathPrefix="/2jmj7l5rSw0yVb%2FvlWAYkK%2FYBwk%3D" />
           </intent-filter>
      </activity>
    </application>
</manifest>
